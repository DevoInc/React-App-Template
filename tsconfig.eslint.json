{
  // this file is so we can lint files other than the ones included in tsconfig.json
  // extend your base config so you don't have to redefine your compilerOptions
  "extends": "./tsconfig.json",
  "include": [
    "src",
    "test",
    ".eslintrc.cjs",
    "eslint.config.mjs",
    ".babelrc",
    "webpack/eslint.config.mjs",
    "jest.config.ts",
    "webpack"
  ]
}
